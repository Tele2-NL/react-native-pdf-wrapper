{
  "name": "@tele2/react-native-pdf-wrapper",
  "description": "Easily show PDFs in React Native.",
  "version": "0.0.3",
  "author": {
    "name": "Lucas Bento",
    "email": "lucas.bento@tele2.com",
    "url": "https://github.com/lucasbento"
  },
  "bugs": "https://github.com/Tele2-NL/react-native-pdf-wrapper/issues",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-preset-react-native": "^4.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "^23.4.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-native": "^0.56.0",
    "react-native-pdf": "^5.0.1",
    "rn-fetch-blob": "^0.10.12"
  },
  "homepage": "https://github.com/Tele2-NL/react-native-pdf-wrapper#readme",
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "!src/index.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/tests/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|rn-fetch-blob|react-native-pdf)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "keywords": [
    "android",
    "ios",
    "react",
    "react-native",
    "pdf",
    "pdf-viewer"
  ],
  "license": "MIT",
  "main": "./src/index.js",
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-native": "*",
    "react-native-pdf": ">=5.0.1",
    "rn-fetch-blob": ">=0.10.12"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/Tele2-NL/react-native-pdf-wrapper"
  },
  "scripts": {
    "ci": "npm run lint && npm run test -- --runInBand",
    "lint": "eslint ./src",
    "test": "jest"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1"
  }
}
